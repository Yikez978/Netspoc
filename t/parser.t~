#!/usr/bin/perl

use strict;
use Test::More;
use Test::Differences;
use lib 't';
use Test_Netspoc;

my ($title, $in, $out1, $head1, $out2, $head2, $out3, $head3);

############################################################
$title = "Unknown model for managed host";
############################################################

$in = <<END;
router:R = {
 managed; 
 model = foo;
}
END

$out1 = <<END;
Error: Unknown router model at line 3 of STDIN
END

eq_or_diff(compile_err($in), $out1, $title);

############################################################
done_testing;
